{
  "annotations": {"list": []},
  "editable": true,
  "graphTooltip": 1,
  "id": null,
  "panels": [
    {
      "title": "Request Rate by Service",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "datasource": {"type": "prometheus", "uid": ""},
      "targets": [
        {
          "expr": "sum by (service_name) (rate(span_metrics_calls_total{span_kind=\"SPAN_KIND_SERVER\"}[5m]))",
          "legendFormat": "{{service_name}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Error Rate % by Service",
      "type": "stat",
      "gridPos": {"h": 4, "w": 12, "x": 12, "y": 0},
      "datasource": {"type": "prometheus", "uid": ""},
      "targets": [
        {
          "expr": "sum by (service_name) (rate(span_metrics_calls_total{span_kind=\"SPAN_KIND_SERVER\", status_code=\"STATUS_CODE_ERROR\"}[5m])) / sum by (service_name) (rate(span_metrics_calls_total{span_kind=\"SPAN_KIND_SERVER\"}[5m]))",
          "legendFormat": "{{service_name}}",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "orientation": "horizontal",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 0.01},
              {"color": "red", "value": 0.05}
            ]
          }
        }
      }
    },
    {
      "title": "P95 Latency by Service",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "datasource": {"type": "prometheus", "uid": ""},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (service_name, le) (rate(span_metrics_duration_milliseconds_bucket{span_kind=\"SPAN_KIND_SERVER\"}[5m])))",
          "legendFormat": "{{service_name}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {"unit": "s"}
      }
    },
    {
      "title": "Recent Traces",
      "type": "table",
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 12},
      "datasource": {"type": "tempo", "uid": "tempo"},
      "targets": [
        {"queryType": "nativeSearch", "limit": 20, "refId": "A"}
      ]
    },
    {
      "title": "Logs",
      "type": "logs",
      "gridPos": {"h": 10, "w": 24, "x": 0, "y": 20},
      "datasource": {"type": "loki", "uid": "loki"},
      "targets": [
        {"expr": "{job=\"docker\"} | json", "refId": "A"}
      ],
      "options": {"showTime": true, "sortOrder": "Descending", "enableLogDetails": true}
    }
  ],
  "schemaVersion": 39,
  "tags": ["demo"],
  "templating": {"list": []},
  "time": {"from": "now-15m", "to": "now"},
  "timezone": "browser",
  "title": "Service Overview",
  "uid": "service-overview",
  "version": 1
}
